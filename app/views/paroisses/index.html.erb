<h1>Listing paroisses</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @paroisses.each do |paroiss| %>
      <tr>
        <td><%= paroiss.nom %></td>
        <td><%= link_to 'Montrer', paroiss %></td>
        <td><%= link_to 'Editer', edit_paroiss_path(paroiss) %></td>
        <td><%= link_to 'Supprimer', paroiss, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nouvelle paroisse', new_paroiss_path %>
